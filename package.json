{
  "name": "kyper-tessellate",
  "version": "0.0.1",
  "description": "Code project building and mangagement platform.",
  "author": "Kyper Digital Inc.",
  "keywords": [
    "kyper",
    "matter",
    "kyper-matter",
    "grout",
    "kyper-grout",
    "devshare"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kypertech/tessellate.git"
  },
  "private": false,
  "license": "MIT",
  "engines": {
    "node": ">=5.x"
  },
  "main": "dist/server.js",
  "scripts": {
    "copy-views": "cp -a src/app/views dist/app",
    "copy-assets": "cp -a public dist/public",
    "copy": "mkdir -p dist && npm run copy-views && npm run copy-assets",
    "pre-deploy": "node ./bin/pre-deploy",
    "build": "npm run copy && babel src --out-dir dist",
    "watch": "npm run build -- --watch",
    "start": "NODE_ENV=development ./node_modules/.bin/nodemon ./dist/server.js",
    "production": "NODE_ENV=production ./node_modules/.bin/forever -m 5 ./dist/server.js",
    "debug": "NODE_ENV=development ./node_modules/.bin/nodemon --debug ./dist/server.js",
    "test": "NODE_ENV=test mocha -R spec --compilers js:babel-core/register ./test/**/*.spec.js",
    "docs:clean": "rimraf _book",
    "docs:prepare": "gitbook install",
    "docs:build": "npm run docs:prepare && gitbook build -g kypertech/tessellate",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "docs:publish": "npm run docs:clean && npm run docs:build && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && touch .nojekyll && git add . && git commit -am 'update book' && git push git@github.com:kypertech/tessellate gh-pages --force"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "body-parser": "~1.14.1",
    "co-express": "~1.2.1",
    "compression": "~1.6.0",
    "connect-flash": "~0.1.1",
    "connect-mongo": "~0.8.2",
    "cookie-parser": "~1.4.0",
    "cookie-session": "~1.2.0",
    "cors": "^2.7.1",
    "express": "~4.13.3",
    "express-jwt": "^3.3.0",
    "express-session": "~1.12.1",
    "express-stormpath": "^2.3.7",
    "firebase-token-generator": "^2.0.0",
    "forever": "~0.15.1",
    "jsonwebtoken": "^5.5.4",
    "lodash": "^4.1.0",
    "method-override": "~2.3.5",
    "mongoose": "~4.2.7",
    "morgan": "~1.6.1",
    "multer": "~1.1.0",
    "notifier": "~0.1.7",
    "oauthio": "^0.3.3",
    "object-assign": "~4.0.1",
    "only": "0.0.2",
    "passport": "~0.3.2",
    "passport-jwt": "^1.2.1",
    "passport-local": "~1.0.0",
    "rimraf": "^2.5.1",
    "s3": "^4.4.0",
    "simple-oauth2": "^0.5.1",
    "stormpath": "^0.17.1",
    "swig": "~1.4.2",
    "view-helpers": "~0.1.5",
    "winston": "~2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "gitbook-cli": "^1.0.1",
    "mocha": "^2.4.5",
    "nodemon": "*"
  }
}
